<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">

	<application>
		<locale-config>
			<default-locale>es</default-locale>
		</locale-config>

		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>


		<navigation-rule>
			<from-view-id>*</from-view-id>
			<navigation-case>
				<from-action>home</from-action>
				<to-view-id>/faces/home.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_roles</from-action>
				<to-view-id>/faces/pages/gestionar_roles.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_proyectos</from-action>
				<to-view-id>/faces/pages/gestionar_proyectos.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_versiones</from-action>
				<to-view-id>/faces/pages/gestionar_versiones.xhtml</to-view-id>
			</navigation-case>	
			<navigation-case>
				<from-action>gestionar_tipoRecurso</from-action>
				<to-view-id>/faces/pages/gestionar_tipoRecurso.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_personas</from-action>
				<to-view-id>/faces/pages/gestionar_personas.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_tipo_documentos</from-action>
				<to-view-id>/faces/pages/gestionar_tipo_documentos.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_recursos</from-action>
				<to-view-id>/faces/pages/gestionar_recursos.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_documentos</from-action>
				<to-view-id>/faces/pages/gestionar_documentos.xhtml</to-view-id>
			</navigation-case>		
			<navigation-case>
				<from-action>gestionar_persona_recurso</from-action>
				<to-view-id>/faces/pages/gestionar_persona_recurso.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_observaciones</from-action>
				<to-view-id>/faces/pages/gestionar_observaciones.xhtml</to-view-id>
			</navigation-case>
			<navigation-case>
				<from-action>gestionar_historial_actividades</from-action>
				<to-view-id>/faces/pages/gestionar_historial_actividades.xhtml</to-view-id>
			</navigation-case>
		</navigation-rule>

	</application>

</faces-config>

